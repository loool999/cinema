#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/amych/proxy-v5/node_modules/.pnpm/@tomphttp+bare-server-node@_e7b728702e2d8d3493645e8c159ad72e/node_modules/@tomphttp/bare-server-node/node_modules:/mnt/c/Users/amych/proxy-v5/node_modules/.pnpm/@tomphttp+bare-server-node@_e7b728702e2d8d3493645e8c159ad72e/node_modules/@tomphttp/node_modules:/mnt/c/Users/amych/proxy-v5/node_modules/.pnpm/@tomphttp+bare-server-node@_e7b728702e2d8d3493645e8c159ad72e/node_modules:/mnt/c/Users/amych/proxy-v5/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/amych/proxy-v5/node_modules/.pnpm/@tomphttp+bare-server-node@_e7b728702e2d8d3493645e8c159ad72e/node_modules/@tomphttp/bare-server-node/node_modules:/mnt/c/Users/amych/proxy-v5/node_modules/.pnpm/@tomphttp+bare-server-node@_e7b728702e2d8d3493645e8c159ad72e/node_modules/@tomphttp/node_modules:/mnt/c/Users/amych/proxy-v5/node_modules/.pnpm/@tomphttp+bare-server-node@_e7b728702e2d8d3493645e8c159ad72e/node_modules:/mnt/c/Users/amych/proxy-v5/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@tomphttp+bare-server-node@_e7b728702e2d8d3493645e8c159ad72e/node_modules/@tomphttp/bare-server-node/bin.js" "$@"
else
  exec node  "$basedir/../.pnpm/@tomphttp+bare-server-node@_e7b728702e2d8d3493645e8c159ad72e/node_modules/@tomphttp/bare-server-node/bin.js" "$@"
fi
